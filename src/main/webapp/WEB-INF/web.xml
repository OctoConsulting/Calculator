 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Calculator</display-name>
  <servlet>
	<servlet-name>CalculateServlet</servlet-name>
	<servlet-class>com.octo.demo.CalculateServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>CalculateServlet</servlet-name>
	<url-pattern>/CalculateServlet</url-pattern>
</servlet-mapping>
 <welcome-file-list>
    <welcome-file>calculate.jsp</welcome-file>
  </welcome-file-list>
</web-app>
